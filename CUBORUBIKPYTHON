    ((f_azul[2][0][2]==17 and f_naranja[2][0][0]==24 and f_amarillo[0][0][2]==47) or (f_azul[2][0][2]==47 and f_naranja[2][0][0]==17 and f_amarillo[0][0][2]==24) or (f_azul[2][0][2]==24 and f_naranja[2][0][0]==47 and f_amarillo[0][0][2]==17))
