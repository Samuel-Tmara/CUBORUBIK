#3
#Seguidamente se sigue a buscar y encontrar el valor 3 y ubicarlo donde tiene que estar en la cruz y así pasa igual con el valor 5 y 1
def cruz_3(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo):
    if 3 in fila1_a:
        print("Está en la cara azul")
        while f_azul[1][0][0] != 3:
            direccion_horaria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Gira direccion horaria")
        izquierda_arriba(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira izqueirda-arriba")
        while f_azul[0][0][1] != 10:
            direccion_horaria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Gira direccion horaria")
    elif 3 in fila2_a:
        print("Está en la cara azul")
        while f_azul[1][0][0] != 3:
            direccion_horaria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Gira direccion horaria")
        izquierda_arriba(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira izqueirda-arriba")
        while f_azul[0][0][1] != 10:
            direccion_horaria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Gira direccion horaria")
    elif 3 in fila3_a:
        print("Está en la cara azul")
        while f_azul[1][0][0] != 3:
            direccion_horaria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Gira direccion horaria")
        izquierda_arriba(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira izqueirda-arriba")
        while f_azul[0][0][1] != 10:
            direccion_horaria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Gira direccion horaria")

    if 3 in fila1_r:
        print("Está en cara rojo")
        while f_rojo[2][0][1] != 3:
            izquierda_abajo(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Gira izquierda-abajo")
        abajo_derecha(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira abajo-derecha")
        while f_azul[1][0][0] != 3:
            direccion_horaria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Gira direccion horaria")
        izquierda_arriba(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira izquierda-arriba")

        while f_azul[0][0][1] != 10:
            direccion_horaria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Gira direccion horaria")
    elif 3 in fila2_r:
        print("Está en cara rojo")
        while f_rojo[2][0][1] != 3:
            izquierda_abajo(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Gira izquierda-abajo")
        abajo_derecha(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira abajo-derecha")
        while f_azul[1][0][0] != 3:
            direccion_horaria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Gira direccion horaria")
        izquierda_arriba(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira izquierda-arriba")
        while f_azul[0][0][1] != 10:
            direccion_horaria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Gira direccion horaria")
    elif 3 in fila3_r:
        print("Está en cara rojo")
        while f_rojo[2][0][1] != 3:
            izquierda_abajo(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Gira izquierda-abajo")
        abajo_derecha(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira abajo-derecha")
        while f_azul[1][0][0] != 3:
            direccion_horaria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Gira direccion horaria")
        izquierda_arriba(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira izquierda-arriba")
        while f_azul[0][0][1] != 10:
            direccion_horaria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Gira direccion horaria")

    if 3 in fila1_b:
        print("Está en cara blanco")
        while f_blanco[1][0][0] != 3:
            arriba_derecha(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Gira arriba-derecha")
        izquierda_abajo(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira izquierda-abajo")
        while f_blanco[2][0][1] != 7:
            arriba_derecha(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print(" Gira arriba-derecha")
        izquierda_arriba(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira izquierda-arriba")
    elif 3 in fila2_b:
        print("Está en cara blanco")
        while f_blanco[1][0][0] != 3:
            arriba_derecha(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print(" Gira arriba-derecha")
        izquierda_abajo(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira izquierda-abajo")
        while f_blanco[2][0][1] != 7:
