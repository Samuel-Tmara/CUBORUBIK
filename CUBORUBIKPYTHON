#5
def cruz_5(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo):
    if 5 in fila1_b:
        print("Esta en la cara blanca ")
        direccion_horaria_verde(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira direccion horaria verde")
        abajo_izquierda(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira abajo izquierda")
        derecha_arriba(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira derecha arriba")
        derecha_arriba(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira derecha arriba ")
    elif 5 in fila2_b:
        print("Esta en la cara blanca ")
        direccion_horaria_verde(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira direccion horaria verde")
        abajo_izquierda(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira abajo izquierda")
        derecha_arriba(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira derecha arriba")
        derecha_arriba(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira derecha arriba ")
    elif 5 in fila3_b:
        print("Esta en la cara blanca ")
        direccion_horaria_verde(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira direccion horaria verde")
        abajo_izquierda(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira abajo izquierda")
        derecha_arriba(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira derecha arriba")
        derecha_arriba(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira derecha arriba ")

    if 5 in fila1_a:
        print("Está en la cara azul")
        while f_azul[1][0][2] != 5:
            direccion_horaria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Gira horaria")
        derecha_arriba(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira derecha arriba")
        while f_azul[0][0][1] != 10:
            direccion_horaria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Gira direccion horario")
    elif 5 in fila2_a:
        print("Está en la cara azul")
        while f_azul[1][0][2] != 5:
            direccion_horaria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Gira horaria")
        derecha_arriba(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira derecha arriba")
        while f_azul[0][0][1] != 10:
            direccion_horaria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Gira direccion horario")
    elif 5 in fila3_a:
        print("Está en la cara azul")
        while f_azul[1][0][2] != 5:
            direccion_horaria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Gira horaria")
        derecha_arriba(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira derecha arriba")
        while f_azul[0][0][1] != 10:
            direccion_horaria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Gira direccion horario")

    if 5 in fila1_n:
        print("Está en la cara naranja")
        while f_azul[2][0][1] != 5:
            derecha_arriba(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Gira derecha arriba")
        abajo_izquierda(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("gira abajo izqueirda")
        direccion_horaria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("gira direccion horaria ")
        derecha_arriba(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("gira derecha arriba")
        direccion_horaria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("gira direccion horaria")
    elif 5 in fila2_n:
        while f_azul[2][0][1] != 5:
            derecha_arriba(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Gira derecha arriba")
        abajo_izquierda(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("gira abajo izqueirda")
        direccion_horaria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
