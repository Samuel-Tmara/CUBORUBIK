'''GIRO DIRECCIÓN ANTIHORARIA'''

def direccion_antihoraria (f_blanco,f_azul,f_naranja,f_verde,f_rojo,f_amarillo):
    f_blanco_da=[[[],[],[]],[[],[],[]],[[],[],[]]]
    f_blanco_da[0][0]=f_blanco[0][0] [0]    #0
    f_blanco_da[0][1]=f_blanco[0][0] [1]    #1
    f_blanco_da[0][2]=f_blanco[0][0] [2]    #2
    f_blanco_da[1][0]=f_blanco[1][0] [0]    #3
    f_blanco_da[1][1]=f_blanco[1][0] [1]    #4
    f_blanco_da[1][2]=f_blanco[1][0] [2]    #5
    f_blanco_da[2][0]=f_naranja[0][0] [0]      #44
    f_blanco_da[2][1]=f_naranja[1][0] [0]      #41
    f_blanco_da[2][2]=f_naranja[2][0] [0]      #38
    print("Cara blanca: ",f_blanco_da)
    f_azul_da=[[[],[],[]],[[],[],[]],[[],[],[]]]########
    f_azul_da[0][0]=f_azul[0][0] [2]    #15
    f_azul_da[0][1]=f_azul[1][0] [2]    #12
    f_azul_da[0][2]=f_azul[2][0] [2]    #9
    f_azul_da[1][0]=f_azul[0][0] [1]    #16
    f_azul_da[1][1]=f_azul[1][0] [1]    #13
    f_azul_da[1][2]=f_azul[2][0] [1]    #10
    f_azul_da[2][0]=f_azul[0][0] [0]    #17
    f_azul_da[2][1]=f_azul[1][0] [0]    #14
    f_azul_da[2][2]=f_azul[2][0] [0]    #11
    print("Cara azul: ", f_azul_da)
    f_naranja_da=[[[],[],[]],[[],[],[]],[[],[],[]]]########
    f_naranja_da[0][0]=f_amarillo[0][0] [2]    #6
    f_naranja_da[0][1]=f_naranja[0][0] [1]   #19
    f_naranja_da[0][2]=f_naranja[0][0] [2]   #20
    f_naranja_da[1][0]=f_amarillo[0][0] [1]    #7
    f_naranja_da[1][1]=f_naranja[1][0] [1]   #22
    f_naranja_da[1][2]=f_naranja[1][0] [2]   #23
    f_naranja_da[2][0]=f_amarillo[0][0] [0]    #8
    f_naranja_da[2][1]=f_naranja[2][0] [1]   #25
    f_naranja_da[2][2]=f_naranja[2][0] [2]   #26
    print("Cara naranja: ", f_naranja_da)
    print("Cara verde: ",f_verde)
    f_rojo_da=[[[],[],[]],[[],[],[]],[[],[],[]]]########
    f_rojo_da[0][0]=f_rojo[0][0] [0]        #36
    f_rojo_da[0][1]=f_rojo[0][0] [1]        #37
    f_rojo_da[0][2]=f_blanco[2][0] [2]    #45
    f_rojo_da[1][0]=f_rojo[1][0] [0]        #39
    f_rojo_da[1][1]=f_rojo[1][0] [1]        #40
    f_rojo_da[1][2]=f_blanco[2][0] [1]    #46
    f_rojo_da[2][0]=f_rojo[2][0] [0]        #42
    f_rojo_da[2][1]=f_rojo[2][0] [1]        #43
    f_rojo_da[2][2]=f_blanco[2][0] [0]    #47
    print("Cara roja: ",f_rojo_da)
    f_amarillo_da=[[[],[],[]],[[],[],[]],[[],[],[]]]########
    f_amarillo_da[0][0]=f_rojo[0][0] [2]   #24
    f_amarillo_da[0][1]=f_rojo[1][0] [2]   #21
    f_amarillo_da[0][2]=f_rojo[2][0] [2]   #18
    f_amarillo_da[1][0]=f_amarillo[1][0] [0]  #48
    f_amarillo_da[1][1]=f_amarillo[1][0] [1]  #49
    f_amarillo_da[1][2]=f_amarillo[1][0] [2]  #50
    f_amarillo_da[2][0]=f_amarillo[2][0] [0]  #51
    f_amarillo_da[2][1]=f_amarillo[2][0] [1]  #52
    f_amarillo_da[2][2]=f_amarillo[2][0] [2]  #53
    print("Cara amarilla: ",f_amarillo_da)

    f_blanco[0][0] [0] = f_blanco_da[0][0]
    f_blanco[0][0] [1] = f_blanco_da[0][1]
    f_blanco[0][0] [2] = f_blanco_da[0][2]
    f_blanco[1][0] [0] = f_blanco_da[1][0]
    f_blanco[1][0] [1] = f_blanco_da[1][1]
    f_blanco[1][0] [2] = f_blanco_da[1][2]
    f_blanco[2][0] [0] = f_blanco_da[2][0]
    f_blanco[2][0] [1] = f_blanco_da[2][1]
    f_blanco[2][0] [2] = f_blanco_da[2][2]########
    f_azul[0][0] [0] = f_azul_da[0][0]
    f_azul[0][0] [1] = f_azul_da[0][1]
    f_azul[0][0] [2] = f_azul_da[0][2]
    f_azul[1][0] [0] = f_azul_da[1][0]
    f_azul[1][0] [1] = f_azul_da[1][1]
    f_azul[1][0] [2] = f_azul_da[1][2]
    f_azul[2][0] [0] = f_azul_da[2][0]
    f_azul[2][0] [1] = f_azul_da[2][1]
    f_azul[2][0] [2] = f_azul_da[2][2]########
    f_naranja[0][0] [0] = f_naranja_da[0][0]
    f_naranja[0][0] [1] = f_naranja_da[0][1]
    f_naranja[0][0] [2] = f_naranja_da[0][2]
    f_naranja[1][0] [0] = f_naranja_da[1][0]
    f_naranja[1][0] [1] = f_naranja_da[1][1]
    f_naranja[1][0] [2] = f_naranja_da[1][2]
    f_naranja[2][0] [0] = f_naranja_da[2][0]
    f_naranja[2][0] [1] = f_naranja_da[2][1]
    f_naranja[2][0] [2] = f_naranja_da[2][2]########
    f_amarillo[0][0] [0] = f_amarillo_da[0][0]
    f_amarillo[0][0] [1] = f_amarillo_da[0][1]
    f_amarillo[0][0] [2] = f_amarillo_da[0][2]
    f_amarillo[1][0] [0] = f_amarillo_da[1][0]
    f_amarillo[1][0] [1] = f_amarillo_da[1][1]
    f_amarillo[1][0] [2] = f_amarillo_da[1][2]
    f_amarillo[2][0] [0] = f_amarillo_da[2][0]
    f_amarillo[2][0] [1] = f_amarillo_da[2][1]
    f_amarillo[2][0] [2] = f_amarillo_da[2][2]########
    f_rojo[0][0] [0] = f_rojo_da[0][0]
