def cruz_1(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo):
    if 1 in fila1_b:
        print("Esta armada la cruz")
    elif 1 in fila2_b:
        print("Está en la cara blanca")
    elif 1 in fila3_b:
        print("Está en la cara blanca")

    if 1 in fila1_a:
        print("Está en la cara azul")
        while f_azul[2][0][1] != 1:
            direccion_horaria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("gira direccion horario")
        abajo_derecha(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print(" gira abajo derecha")
        derecha_abajo(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("gira derecha abajo")
        direccion_horaria_verde (f_blanco,f_azul,f_naranja,f_verde,f_rojo,f_amarillo)
        print("gira direccion horaria verde")
        derecha_arriba(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("gira derecha arriba")
        while f_azul[0][0][1] != 10:
            direccion_horaria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("gira direccion horaria")
    elif 1 in fila2_a:
        print("Está en la cara azul")
        while f_azul[2][0][1] != 1:
            direccion_horaria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("gira direccion horario")
        abajo_derecha(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print(" gira abajo derecha")
        derecha_abajo(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("gira derecha abajo")
        direccion_horaria_verde (f_blanco,f_azul,f_naranja,f_verde,f_rojo,f_amarillo)
        print("gira direccion horaria verde")
        derecha_arriba(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
