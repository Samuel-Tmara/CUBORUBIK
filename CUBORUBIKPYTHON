"""CRUZ BLANCA"""

#7
#Se crea una función con un algoritmo que desarrollamos en conjunto para que el valor número 7 lo podamos ubicar al comienzo de la cruz

def cruz_7 (f_blanco,f_azul,f_naranja,f_verde,f_rojo,f_amarillo):
    if 7 in fila1_b:
        print("Está en la cara blanca")
        while f_azul[0][0][1] != 10:
            arriba_derecha(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Gira arriba a la derecha")
    elif 7 in fila2_b:
        print("Está en la cara blanca")
        while f_azul[0][0][1] != 10:
            arriba_derecha(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Gira arriba a la derecha")
    elif 7 in fila3_b:
        print("Está en la cara blanca")
        while f_azul[0][0][1] != 10:
            arriba_derecha(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Gira arriba a la derecha")

    if 7 in fila1_a:
        print("Está en la cara azul")
        while f_azul[2][0][1] != 7:
            direccion_horaria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Gira dirección horaria")
        abajo_derecha(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira abajo-derecha")
        derecha_arriba(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira derecha-arriba")
        direccion_antihoraria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gra dirección antihoraria")
    elif 7 in fila2_a:
        print("Está en la cara azul")
        while f_azul[2][0][1] != 7:
            direccion_horaria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Gira dirección horaria")
        abajo_derecha(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira abajo-derecha")
        derecha_arriba(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira derecha-arriba")
        direccion_antihoraria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gra dirección antihoraria")
    elif 7 in fila3_a:
        print("Está en la cara azul")
        while f_azul[2][0][1] != 7:
            direccion_horaria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Gira dirección horaria")
        abajo_derecha(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira abajo-derecha")
        derecha_arriba(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira derecha-arriba")
        direccion_antihoraria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira dirección antihoraria")

    if 7 in fila1_n:
        print("Está en la cara naranja")
        while f_azul[1][0][2] != 10:
            derecha_arriba(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Gira derecha_arriba")
        direccion_antihoraria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira en direccion antihoraria ")
    elif 7 in fila2_n:
        print("Está en la cara naranja")
        while f_azul[1][0][2] != 10:
            derecha_arriba(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Gira derecha-arriba ")
        direccion_antihoraria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira direccion antihoraria")
    elif 7 in fila3_n:
        print("Está en la cara naranja")
        while f_azul[1][0][2] != 10:
            derecha_arriba(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Gira derecha-arriba")
        direccion_antihoraria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira en direccion antihoraria")

    if 7 in fila1_v:
        print("Está en la cara verde")
        while f_verde[2][0][1] != 7:
            direccion_horaria_verde(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Gira direcion horaria-verde")
        abajo_izquierda(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira abajo-izquierda")
        derecha_arriba(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira derecha-arriba")
        direccion_antihoraria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira dieccion antihoraria")
    elif 7 in fila2_v:
        print("Está en la cara verde")
        while f_verde[2][0][1] != 7:
            direccion_horaria_verde(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Gira direcion horaria-verde")
        abajo_izquierda(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira abajo-izquierda")
        derecha_arriba(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira derecha-arriba")
        direccion_antihoraria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira dieccion antihoraria")
    elif 7 in fila3_v:
        print("Está en la cara verde")
        while f_verde[2][0][1] != 7:
            direccion_horaria_verde(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Gira direcion horaria-verde")
        abajo_izquierda(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira abajo-izquierda")
        derecha_arriba(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira derecha-arriba")
        direccion_antihoraria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira dieccion antihoraria")

    if 7 in fila1_r:
        print("Está en la cara rojo")
        while f_azul[1][0][0] != 10:
            izquierda_arriba(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("gira izquierda-arriba")
        direccion_horaria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira direccion horaria")
    elif 7 in fila2_r:
        print("Está en la cara rojo")
        while f_azul[1][0][0] != 10:
            izquierda_arriba(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("gira izquierda-arriba")
        direccion_horaria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira direccion horaria")
    elif 7 in fila3_r:
        print("Está en la cara rojo")
        while f_azul[1][0][0] != 10:
            izquierda_arriba(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("gira izquierda-arriba")
        direccion_horaria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira direccion horaria")

    if 7 in fila1_am:
        print("Está en la cara amarillo")
        while f_azul[2][0][1] != 10:
            abajo_derecha(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Gira abajo-derecha")
        direccion_antihoraria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira direccion antihoraria")
        direccion_antihoraria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira direccion antihoraria")
    elif 7 in fila2_am:
        print("Está en la cara amarillo")
        while f_azul[2][0][1] != 10:
            abajo_derecha(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Gira abajo-derecha")
        direccion_antihoraria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira direccion antihoraria")
        direccion_antihoraria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira direccion antihoraria")
    elif 7 in fila3_am:
        print("Está en la cara amarillo")
        while f_azul[2][0][1] != 10:
            abajo_derecha(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Gira abajo-derecha")
        direccion_antihoraria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira direccion antihoraria")
        direccion_antihoraria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Gira direccion antihoraria")

print(cruz_7 (f_blanco,f_azul,f_naranja,f_verde,f_rojo,f_amarillo))

