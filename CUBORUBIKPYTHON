'''MOVER A LA IZQUIERDA ABAJO'''

def izquierda_abajo (f_blanco,f_azul,f_naranja,f_verde,f_rojo,f_amarillo):
    f_blanco_ld=[[[],[],[]],[[],[],[]],[[],[],[]]]
    f_blanco_ld[0][0]=f_verde[2][0] [2]    #35
    f_blanco_ld[0][1]=f_blanco[0][0] [1]   #1
    f_blanco_ld[0][2]=f_blanco[0][0] [2]   #2
    f_blanco_ld[1][0]=f_verde[1][0] [2]    #32
    f_blanco_ld[1][1]=f_blanco[1][0] [1]   #4
    f_blanco_ld[1][2]=f_blanco[1][0] [2]   #5
    f_blanco_ld[2][0]=f_verde[0][0] [2]    #29
    f_blanco_ld[2][1]=f_blanco[2][0] [1]   #7
    f_blanco_ld[2][2]=f_blanco[2][0] [2]   #8
    print("Cara blanca: ",f_blanco_ld)
    f_azul_ld=[[[],[],[]],[[],[],[]],[[],[],[]]] ########
    f_azul_ld[0][0]=f_blanco[0][0] [0]   #0
    f_azul_ld[0][1]=f_azul[0][0] [1]     #10
    f_azul_ld[0][2]=f_azul[0][0] [2]     #11
    f_azul_ld[1][0]=f_blanco[1][0] [0]   #3
    f_azul_ld[1][1]=f_azul[1][0] [1]     #13
    f_azul_ld[1][2]=f_azul[1][0] [2]     #14
    f_azul_ld[2][0]=f_blanco[2][0] [0]   #6
    f_azul_ld[2][1]=f_azul[2][0] [1]     #16
    f_azul_ld[2][2]=f_azul[2][0] [2]     #17
    print("Cara azul: ", f_azul_ld)
    print("Cara naranja: ", f_naranja)
    f_verde_ld=[[[],[],[]],[[],[],[]],[[],[],[]]] ########
    f_verde_ld[0][0]=f_verde[0][0] [0]      #27
    f_verde_ld[0][1]=f_verde[0][0] [1]      #28
    f_verde_ld[0][2]=f_amarillo[2][0] [0]   #51
    f_verde_ld[1][0]=f_verde[1][0] [0]      #30
    f_verde_ld[1][1]=f_verde[1][0] [1]      #31
    f_verde_ld[1][2]=f_amarillo[1][0] [0]   #48
    f_verde_ld[2][0]=f_verde[2][0] [0]      #33
    f_verde_ld[2][1]=f_verde[2][0] [1]      #34
    f_verde_ld[2][2]=f_amarillo[0][0] [0]   #45
    print("Cara verde: ",f_verde_ld)
    f_rojo_ld=[[[],[],[]],[[],[],[]],[[],[],[]]] ########
    f_rojo_ld[0][0]=f_rojo[2][0] [0]    #42
    f_rojo_ld[0][1]=f_rojo[1][0] [0]    #39
    f_rojo_ld[0][2]=f_rojo[0][0] [0]    #36
    f_rojo_ld[1][0]=f_rojo[2][0] [1]    #43
    f_rojo_ld[1][1]=f_rojo[1][0] [1]    #40
    f_rojo_ld[1][2]=f_rojo[0][0] [1]    #37
    f_rojo_ld[2][0]=f_rojo[2][0] [2]    #44
    f_rojo_ld[2][1]=f_rojo[1][0] [2]    #41
    f_rojo_ld[2][2]=f_rojo[0][0] [2]    #38
    print("Cara roja: ",f_rojo_ld)
    f_amarillo_ld=[[[],[],[]],[[],[],[]],[[],[],[]]] ########
    f_amarillo_ld[0][0]=f_azul[0][0] [0]        #9
    f_amarillo_ld[0][1]=f_amarillo[0][0] [1]    #46
    f_amarillo_ld[0][2]=f_amarillo[0][0] [2]    #47
    f_amarillo_ld[1][0]=f_azul[1][0] [0]        #12
    f_amarillo_ld[1][1]=f_amarillo[1][0] [1]    #49
    f_amarillo_ld[1][2]=f_amarillo[1][0] [2]    #50
    f_amarillo_ld[2][0]=f_azul[2][0] [0]        #15
    f_amarillo_ld[2][1]=f_amarillo[2][0] [1]    #52
    f_amarillo_ld[2][2]=f_amarillo[2][0] [2]    #53
    print("Cara amarilla: ",f_amarillo_ld)

    f_blanco[0][0] [0] = f_blanco_ld[0][0]
    f_blanco[0][0] [1] = f_blanco_ld[0][1]
    f_blanco[0][0] [2] = f_blanco_ld[0][2]
    f_blanco[1][0] [0] = f_blanco_ld[1][0]
    f_blanco[1][0] [1] = f_blanco_ld[1][1]
    f_blanco[1][0] [2] = f_blanco_ld[1][2]
    f_blanco[2][0] [0] = f_blanco_ld[2][0]
