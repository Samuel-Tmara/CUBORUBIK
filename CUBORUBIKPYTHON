    f_blanco[2][0] [0] = f_blanco_dh[2][0]
    f_blanco[2][0] [1] = f_blanco_dh[2][1]
    f_blanco[2][0] [2] = f_blanco_dh[2][2]########
    f_azul[0][0] [0] = f_azul_dh[0][0]
    f_azul[0][0] [1] = f_azul_dh[0][1]
