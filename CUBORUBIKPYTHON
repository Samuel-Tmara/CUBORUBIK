'''GIRO DIRECCIÓN HORARIA VERDE'''

def direccion_horaria_verde (f_blanco,f_azul,f_naranja,f_verde,f_rojo,f_amarillo):
    f_blanco_dhv=[[[],[],[]],[[],[],[]],[[],[],[]]]
    f_blanco_dhv[0][0]=f_naranja[0][0] [2]    #0
    f_blanco_dhv[0][1]=f_naranja[1][0] [2]    #1
    f_blanco_dhv[0][2]=f_naranja[2][0] [2]    #2
    f_blanco_dhv[1][0]=f_blanco[1][0] [0]    #3
    f_blanco_dhv[1][1]=f_blanco[1][0] [1]    #4
    f_blanco_dhv[1][2]=f_blanco[1][0] [2]    #5
    f_blanco_dhv[2][0]=f_blanco[2][0] [0]      #44
    f_blanco_dhv[2][1]=f_blanco[2][0] [1]      #41
    f_blanco_dhv[2][2]=f_blanco[2][0][2]      #38
    print("Cara blanca: ",f_blanco_dhv)
    print("Cara azul: ", f_azul)
    f_naranja_dhv=[[[],[],[]],[[],[],[]],[[],[],[]]]########
    f_naranja_dhv[0][0]=f_naranja[0][0] [0]    #6
    f_naranja_dhv[0][1]=f_naranja[0][0] [1]   #19
    f_naranja_dhv[0][2]=f_amarillo[2][0] [2]   #20
    f_naranja_dhv[1][0]=f_naranja[1][0] [0]    #7
    f_naranja_dhv[1][1]=f_naranja[1][0] [1]   #22
    f_naranja_dhv[1][2]=f_amarillo[2][0] [1]   #23
    f_naranja_dhv[2][0]=f_naranja[2][0] [0]    #8
    f_naranja_dhv[2][1]=f_naranja[2][0] [1]   #25
    f_naranja_dhv[2][2]=f_amarillo[2][0] [0]   #26
    print("Cara naranja: ", f_naranja_dhv)
    f_verde_dhv=[[[],[],[]],[[],[],[]],[[],[],[]]]########
    f_verde_dhv[0][0]=f_verde[2][0] [0]        #36
    f_verde_dhv[0][1]=f_verde[1][0] [0]        #37
    f_verde_dhv[0][2]=f_verde[0][0] [0]    #45
    f_verde_dhv[1][0]=f_verde[2][0] [1]        #39
    f_verde_dhv[1][1]=f_verde[1][0] [1]        #40
    f_verde_dhv[1][2]=f_verde[0][0] [1]    #46
    f_verde_dhv[2][0]=f_verde[2][0] [2]        #42
    f_verde_dhv[2][1]=f_verde[1][0] [2]        #43
    f_verde_dhv[2][2]=f_verde[0][0] [2]    #47
    print("Cara verde: ",f_verde_dhv)
    f_rojo_dhv=[[[],[],[]],[[],[],[]],[[],[],[]]]########
    f_rojo_dhv[0][0]=f_blanco[0][0] [2]        #36
    f_rojo_dhv[0][1]=f_rojo[0][0] [1]        #37
    f_rojo_dhv[0][2]=f_rojo[0][0] [2]    #45
    f_rojo_dhv[1][0]=f_blanco[0][0] [1]        #39
    f_rojo_dhv[1][1]=f_rojo[1][0] [1]        #40
    f_rojo_dhv[1][2]=f_rojo[1][0] [2]    #46
    f_rojo_dhv[2][0]=f_blanco[0][0] [0]        #42
    f_rojo_dhv[2][1]=f_rojo[2][0] [1]        #43
    f_rojo_dhv[2][2]=f_rojo[2][0] [2]    #47
    print("Cara roja: ",f_rojo_dhv)
    f_amarillo_dhv=[[[],[],[]],[[],[],[]],[[],[],[]]]########
    f_amarillo_dhv[0][0]=f_amarillo[0][0] [0]   #24
    f_amarillo_dhv[0][1]=f_amarillo[0][0] [1]   #21
    f_amarillo_dhv[0][2]=f_amarillo[0][0] [2]   #18
    f_amarillo_dhv[1][0]=f_amarillo[1][0] [0]  #48
    f_amarillo_dhv[1][1]=f_amarillo[1][0] [1]  #49
    f_amarillo_dhv[1][2]=f_amarillo[1][0] [2]  #50
    f_amarillo_dhv[2][0]=f_rojo[0][0] [0]  #51
    f_amarillo_dhv[2][1]=f_rojo[1][0] [0]  #52
    f_amarillo_dhv[2][2]=f_rojo[2][0] [0]  #53
    print("Cara amarilla: ",f_amarillo_dhv)

    f_blanco[0][0] [0] = f_blanco_dhv[0][0]
    f_blanco[0][0] [1] = f_blanco_dhv[0][1]
    f_blanco[0][0] [2] = f_blanco_dhv[0][2]
    f_blanco[1][0] [0] = f_blanco_dhv[1][0]
    f_blanco[1][0] [1] = f_blanco_dhv[1][1]
    f_blanco[1][0] [2] = f_blanco_dhv[1][2]
    f_blanco[2][0] [0] = f_blanco_dhv[2][0]
    f_blanco[2][0] [1] = f_blanco_dhv[2][1]
    f_blanco[2][0] [2] = f_blanco_dhv[2][2]########
    f_amarillo[0][0] [0] = f_amarillo_dhv[0][0]
    f_amarillo[0][0] [1] = f_amarillo_dhv[0][1]
    f_amarillo[0][0] [2] = f_amarillo_dhv[0][2]
    f_amarillo[1][0] [0] = f_amarillo_dhv[1][0]
    f_amarillo[1][0] [1] = f_amarillo_dhv[1][1]
    f_amarillo[1][0] [2] = f_amarillo_dhv[1][2]
    f_amarillo[2][0] [0] = f_amarillo_dhv[2][0]
    f_amarillo[2][0] [1] = f_amarillo_dhv[2][1]
    f_amarillo[2][0] [2] = f_amarillo_dhv[2][2]########
    f_naranja[0][0] [0] = f_naranja_dhv[0][0]
    f_naranja[0][0] [1] = f_naranja_dhv[0][1]
    f_naranja[0][0] [2] = f_naranja_dhv[0][2]
    f_naranja[1][0] [0] = f_naranja_dhv[1][0]
    f_naranja[1][0] [1] = f_naranja_dhv[1][1]
    f_naranja[1][0] [2] = f_naranja_dhv[1][2]
    f_naranja[2][0] [0] = f_naranja_dhv[2][0]
    f_naranja[2][0] [1] = f_naranja_dhv[2][1]
    f_naranja[2][0] [2] = f_naranja_dhv[2][2]########
    f_verde[0][0] [0] = f_verde_dhv[0][0]
    f_verde[0][0] [1] = f_verde_dhv[0][1]
    f_verde[0][0] [2] = f_verde_dhv[0][2]
    f_verde[1][0] [0] = f_verde_dhv[1][0]
    f_verde[1][0] [1] = f_verde_dhv[1][1]
    f_verde[1][0] [2] = f_verde_dhv[1][2]
    f_verde[2][0] [0] = f_verde_dhv[2][0]
    f_verde[2][0] [1] = f_verde_dhv[2][1]
    f_verde[2][0] [2] = f_verde_dhv[2][2]########
    f_rojo[0][0] [0] = f_rojo_dhv[0][0]
    f_rojo[0][0] [1] = f_rojo_dhv[0][1]
    f_rojo[0][0] [2] = f_rojo_dhv[0][2]
    f_rojo[1][0] [0] = f_rojo_dhv[1][0]
    f_rojo[1][0] [1] = f_rojo_dhv[1][1]
    f_rojo[1][0] [2] = f_rojo_dhv[1][2]
    f_rojo[2][0] [0] = f_rojo_dhv[2][0]
    f_rojo[2][0] [1] = f_rojo_dhv[2][1]
    f_rojo[2][0] [2] = f_rojo_dhv[2][2]


'''GIRO DIRECCIÓN ANTIHORARIA VERDE'''

def direccion_antihoraria_verde (f_blanco,f_azul,f_naranja,f_verde,f_rojo,f_amarillo):
    f_blanco_dav=[[[],[],[]],[[],[],[]],[[],[],[]]]
    f_blanco_dav[0][0]=f_rojo[2][0][0]     #0
    f_blanco_dav[0][1]=f_rojo[1][0][0]     #1
    f_blanco_dav[0][2]=f_rojo[0][0][0]     #2
    f_blanco_dav[1][0]=f_blanco[1][0][0]   #3
    f_blanco_dav[1][1]=f_blanco[1][0][1]   #4
    f_blanco_dav[1][2]=f_blanco[1][0][2]   #5
    f_blanco_dav[2][0]=f_blanco[2][0][0]   #44
    f_blanco_dav[2][1]=f_blanco[2][0][1]   #41
    f_blanco_dav[2][2]=f_blanco [2][0][2]  #38
    print("Cara blanca: ",f_blanco_dav)
    print("Cara azul: ", f_azul)
    f_naranja_dav=[[[],[],[]],[[],[],[]],[[],[],[]]]########
    f_naranja_dav[0][0]=f_naranja[0][0][0]    #6
    f_naranja_dav[0][1]=f_naranja[0][0][1]   #19
    f_naranja_dav[0][2]=f_blanco[0][0][0]   #20
    f_naranja_dav[1][0]=f_naranja[1][0][0]    #7
    f_naranja_dav[1][1]=f_naranja[1][0][1]   #22
    f_naranja_dav[1][2]=f_blanco[0][0][1]   #23
    f_naranja_dav[2][0]=f_naranja[2][0][0]    #8
    f_naranja_dav[2][1]=f_naranja[2][0][1]   #25
    f_naranja_dav[2][2]=f_blanco[0][0][2]   #26
    print("Cara naranja: ", f_naranja_dav)
    f_verde_dav=[[[],[],[]],[[],[],[]],[[],[],[]]]########
    f_verde_dav[0][0]=f_verde[0][0][2]        #36
    f_verde_dav[0][1]=f_verde[1][0][2]        #37
