    f_naranja[2][0][1] = f_naranja_dr[2][1]
    f_naranja[2][0][2] = f_naranja_dr[2][2]########
    f_verde[0][0][0] = f_verde_dr[0][0]
    f_verde[0][0][1] = f_verde_dr[0][1]
