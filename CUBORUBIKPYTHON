    f_amarillo_dl[0][1]=f_amarillo[1][0][2]   #50
    f_amarillo_dl[0][2]=f_amarillo[2][0][2]   #53
    f_amarillo_dl[1][0]=f_amarillo[0][0][1]   #46
    f_amarillo_dl[1][1]=f_amarillo[1][0][1]   #49
    f_amarillo_dl[1][2]=f_amarillo[2][0][1]   #52
