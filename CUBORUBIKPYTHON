fila1_b=[26,25,29]
fila2_b=[52,4,32]           #Estas 3 listas son para crear la cara del cubo (Cada lista es una fila)
fila3_b=[2,46,17]

fila1_a=[27,16,47]
fila2_a=[43,13,7]
fila3_a=[45,37,51]

fila1_n=[24,39,36]
fila2_n=[10,22,5]
fila3_n=[35,30,9]

fila1_v=[0,50,53]
fila2_v=[19,31,28]
fila3_v=[9,21,18]

fila1_r=[33,34,20]
fila2_r=[1,40,48]
fila3_r=[11,12,15]

fila1_am=[44,3,42]
fila2_am=[14,49,23]
fila3_am=[8,14,38]

f_blanco=[[fila1_b],[fila2_b],[fila3_b]]                #Acá unimos las anteriores listas (filas) para formar la cara completa
f_azul=[[fila1_a],[fila2_a],[fila3_a]]
f_naranja=[[fila1_n],[fila2_n],[fila3_n]]
f_verde=[[fila1_v],[fila2_v],[fila3_v]]
f_rojo=[[fila1_r],[fila2_r],[fila3_r]]
f_amarillo=[[fila1_am],[fila2_am],[fila3_am]]

'''MOVER ARRIBA A LA DERECHA'''

def arriba_derecha (f_blanco,f_azul,f_naranja,f_verde,f_rojo,f_amarillo):       #Movimiento arriba (Ubicación respecto al centro azul) a la derecha (Dirección del giro) con respecto al centro azul
    f_blanco_ur=[[[],[],[]],[[],[],[]],[[],[],[]]]              #Se crea una lista vacia de listas que va a ser la nueva posición de las caras del cubo (En este caso el color blanco)
    f_blanco_ur[0][0]=f_blanco[0][0][2]   #2                #De acuerdo a la ubicación en el cubo se le asigna el nuevo valor
    f_blanco_ur[0][1]=f_blanco[1][0][2]   #5
    f_blanco_ur[0][2]=f_blanco[2][0][2]   #8
    f_blanco_ur[1][0]=f_blanco[0][0][1]   #1
    f_blanco_ur[1][1]=f_blanco[1][0][1]   #4
    f_blanco_ur[1][2]=f_blanco[2][0][1]   #7
    f_blanco_ur[2][0]=f_blanco[0][0][0]   #0
    f_blanco_ur[2][1]=f_blanco[1][0][0]   #3
    f_blanco_ur[2][2]=f_blanco[2][0][0]   #6
    print("Cara blanca: ",f_blanco_ur)
    f_azul_ur=[[[],[],[]],[[],[],[]],[[],[],[]]]########
    f_azul_ur[0][0]=f_rojo[0][0][0]     #36
    f_azul_ur[0][1]=f_rojo[0][0][1]     #37
    f_azul_ur[0][2]=f_rojo[0][0][2]     #38
    f_azul_ur[1][0]=f_azul[1][0][0]   #12
    f_azul_ur[1][1]=f_azul[1][0][1]   #13
    f_azul_ur[1][2]=f_azul[1][0][2]   #14
    f_azul_ur[2][0]=f_azul[2][0][0]   #15
    f_azul_ur[2][1]=f_azul[2][0][1]   #16
    f_azul_ur[2][2]=f_azul[2][0][2]   #17
    print("Cara azul: ", f_azul_ur)
    f_naranja_ur = [[[],[],[]],[[],[],[]],[[],[],[]]]########
    f_naranja_ur[0][0]=f_azul[0][0][0]     #36
    f_naranja_ur[0][1]=f_azul[0][0][1]     #37
    f_naranja_ur[0][2]=f_azul[0][0][2]     #38
    f_naranja_ur[1][0]=f_naranja[1][0][0]   #12
    f_naranja_ur[1][1]=f_naranja[1][0][1]   #13
    f_naranja_ur[1][2]=f_naranja[1][0][2]   #14
    f_naranja_ur[2][0]=f_naranja[2][0][0]   #15
    f_naranja_ur[2][1]=f_naranja[2][0][1]   #16
    f_naranja_ur[2][2]=f_naranja[2][0][2]   #17
    print("Cara naranja: ", f_naranja_ur)
    f_verde_ur=[[[],[],[]],[[],[],[]],[[],[],[]]]########
    f_verde_ur[0][0]=f_naranja[0][0][0]   #27
    f_verde_ur[0][1]=f_naranja[0][0][1]   #28
    f_verde_ur[0][2]=f_naranja[0][0][2]   #29
    f_verde_ur[1][0]=f_verde[1][0][0]   #39
    f_verde_ur[1][1]=f_verde[1][0][1]   #40
    f_verde_ur[1][2]=f_verde[1][0][2]   #41
    f_verde_ur[2][0]=f_verde[2][0][0]   #42
    f_verde_ur[2][1]=f_verde[2][0][1]   #43
    f_verde_ur[2][2]=f_verde[2][0][2]   #44
    print("Cara verde: ",f_verde_ur)
    f_rojo_ur=[[[],[],[]],[[],[],[]],[[],[],[]]]########
    f_rojo_ur[0][0]=f_verde[0][0][0]   #27
    f_rojo_ur[0][1]=f_verde[0][0][1]   #28
    f_rojo_ur[0][2]=f_verde[0][0][2]   #29
    f_rojo_ur[1][0]=f_rojo[1][0][0]   #39
    f_rojo_ur[1][1]=f_rojo[1][0][1]   #40
    f_rojo_ur[1][2]=f_rojo[1][0][2]   #41
    f_rojo_ur[2][0]=f_rojo[2][0][0]   #42
    f_rojo_ur[2][1]=f_rojo[2][0][1]   #43
    f_rojo_ur[2][2]=f_rojo[2][0][2]   #44
    print("Cara roja: ",f_rojo_ur)
    print("Cara amarilla: ",f_amarillo)

    f_blanco[0][0][0] = f_blanco_ur[0][0]           #El nuevo valor de la coordenada de esta posición es la que queda como resultado después de hacer el movimiento anterior
    f_blanco[0][0][1] = f_blanco_ur[0][1]
    f_blanco[0][0][2] = f_blanco_ur[0][2]
    f_blanco[1][0][0] = f_blanco_ur[1][0]
    f_blanco[1][0][1] = f_blanco_ur[1][1]
    f_blanco[1][0][2] = f_blanco_ur[1][2]
    f_blanco[2][0][0] = f_blanco_ur[2][0]
    f_blanco[2][0][1] = f_blanco_ur[2][1]
    f_blanco[2][0][2] = f_blanco_ur[2][2]########
    f_azul[0][0][0] = f_azul_ur[0][0]
    f_azul[0][0][1] = f_azul_ur[0][1]
    f_azul[0][0][2] = f_azul_ur[0][2]
    f_azul[1][0][0] = f_azul_ur[1][0]
    f_azul[1][0][1] = f_azul_ur[1][1]
    f_azul[1][0][2] = f_azul_ur[1][2]
    f_azul[2][0][0] = f_azul_ur[2][0]
    f_azul[2][0][1] = f_azul_ur[2][1]
    f_azul[2][0][2] = f_azul_ur[2][2]########
    f_naranja[0][0][0] = f_naranja_ur[0][0]
    f_naranja[0][0][1] = f_naranja_ur[0][1]
    f_naranja[0][0][2] = f_naranja_ur[0][2]
    f_naranja[1][0][0] = f_naranja_ur[1][0]
    f_naranja[1][0][1] = f_naranja_ur[1][1]
    f_naranja[1][0][2] = f_naranja_ur[1][2]
    f_naranja[2][0][0] = f_naranja_ur[2][0]
    f_naranja[2][0][1] = f_naranja_ur[2][1]
    f_naranja[2][0][2] = f_naranja_ur[2][2]########
    f_verde[0][0][0] = f_verde_ur[0][0]
    f_verde[0][0][1] = f_verde_ur[0][1]
    f_verde[0][0][2] = f_verde_ur[0][2]
    f_verde[1][0][0] = f_verde_ur[1][0]
    f_verde[1][0][1] = f_verde_ur[1][1]
    f_verde[1][0][2] = f_verde_ur[1][2]
    f_verde[2][0][0] = f_verde_ur[2][0]
    f_verde[2][0][1] = f_verde_ur[2][1]
    f_verde[2][0][2] = f_verde_ur[2][2]########
    f_rojo[0][0][0] = f_rojo_ur[0][0]
    f_rojo[0][0][1] = f_rojo_ur[0][1]
    f_rojo[0][0][2] = f_rojo_ur[0][2]
    f_rojo[1][0][0] = f_rojo_ur[1][0]
    f_rojo[1][0][1] = f_rojo_ur[1][1]
    f_rojo[1][0][2] = f_rojo_ur[1][2]
    f_rojo[2][0][0] = f_rojo_ur[2][0]
    f_rojo[2][0][1] = f_rojo_ur[2][1]
    f_rojo[2][0][2] = f_rojo_ur[2][2]

'''MOVER ARRIBA A LA IZQUIERDA'''

def arriba_izquierda (f_blanco,f_azul,f_naranja,f_verde,f_rojo,f_amarillo):
    f_blanco_ul=[[[],[],[]],[[],[],[]],[[],[],[]]]
    f_blanco_ul[0][0]=f_blanco[2][0][0]   #6
    f_blanco_ul[0][1]=f_blanco[1][0][0]   #3
    f_blanco_ul[0][2]=f_blanco[0][0][0]   #0
    f_blanco_ul[1][0]=f_blanco[2][0][1]   #7
    f_blanco_ul[1][1]=f_blanco[1][0][1]   #4
    f_blanco_ul[1][2]=f_blanco[0][0][1]   #1
    f_blanco_ul[2][0]=f_blanco[2][0][2]   #8
    f_blanco_ul[2][1]=f_blanco[1][0][2]   #5
    f_blanco_ul[2][2]=f_blanco[0][0][2]   #2
    print("Cara blanca: ",f_blanco_ul)
    f_azul_ul=[[[],[],[]],[[],[],[]],[[],[],[]]]########
    f_azul_ul[0][0]=f_naranja[0][0][0]     #36
    f_azul_ul[0][1]=f_naranja[0][0][1]     #37
    f_azul_ul[0][2]=f_naranja[0][0][2]     #38
    f_azul_ul[1][0]=f_azul[1][0][0]        #12
    f_azul_ul[1][1]=f_azul[1][0][1]        #13
    f_azul_ul[1][2]=f_azul[1][0][2]        #14
    f_azul_ul[2][0]=f_azul[2][0][0]        #15
    f_azul_ul[2][1]=f_azul[2][0][1]        #16
    f_azul_ul[2][2]=f_azul[2][0][2]        #17
    print("Cara azul: ", f_azul_ul)
    f_naranja_ul=[[[],[],[]],[[],[],[]],[[],[],[]]]########
    f_naranja_ul[0][0]=f_verde[0][0][0]     #27
    f_naranja_ul[0][1]=f_verde[0][0][1]     #28
    f_naranja_ul[0][2]=f_verde[0][0][2]     #29
    f_naranja_ul[1][0]=f_naranja[1][0][0]   #21
    f_naranja_ul[1][1]=f_naranja[1][0][1]   #22
    f_naranja_ul[1][2]=f_naranja[1][0][2]   #23
    f_naranja_ul[2][0]=f_naranja[2][0][0]   #24
    f_naranja_ul[2][1]=f_naranja[2][0][1]   #25
    f_naranja_ul[2][2]=f_naranja[2][0][2]   #26
    print("Cara naranja: ", f_naranja_ul)
    f_verde_ul=[[[],[],[]],[[],[],[]],[[],[],[]]]########
    f_verde_ul[0][0]=f_rojo[0][0][0]   #36
    f_verde_ul[0][1]=f_rojo[0][0][1]   #37
    f_verde_ul[0][2]=f_rojo[0][0][2]   #38
    f_verde_ul[1][0]=f_verde[1][0][0]   #30
    f_verde_ul[1][1]=f_verde[1][0][1]   #31
    f_verde_ul[1][2]=f_verde[1][0][2]   #32
    f_verde_ul[2][0]=f_verde[2][0][0]   #33
    f_verde_ul[2][1]=f_verde[2][0][1]   #34
    f_verde_ul[2][2]=f_verde[2][0][2]   #35
    print("Cara verde: ",f_verde_ul)
    f_rojo_ul=[[[],[],[]],[[],[],[]],[[],[],[]]]########
    f_rojo_ul[0][0]=f_azul[0][0][0]   #27
    f_rojo_ul[0][1]=f_azul[0][0][1]   #28
    f_rojo_ul[0][2]=f_azul[0][0][2]   #29
    f_rojo_ul[1][0]=f_rojo[1][0][0]   #39
    f_rojo_ul[1][1]=f_rojo[1][0][1]   #40
    f_rojo_ul[1][2]=f_rojo[1][0][2]   #41
    f_rojo_ul[2][0]=f_rojo[2][0][0]   #42
    f_rojo_ul[2][1]=f_rojo[2][0][1]   #43
    f_rojo_ul[2][2]=f_rojo[2][0][2]   #44
    print("Cara roja: ",f_rojo_ul)
    print("Cara amarilla: ",f_amarillo)

    f_blanco[0][0][0] = f_blanco_ul[0][0]
    f_blanco[0][0][1] = f_blanco_ul[0][1]
    f_blanco[0][0][2] = f_blanco_ul[0][2]
    f_blanco[1][0][0] = f_blanco_ul[1][0]
    f_blanco[1][0][1] = f_blanco_ul[1][1]
    f_blanco[1][0][2] = f_blanco_ul[1][2]
    f_blanco[2][0][0] = f_blanco_ul[2][0]
    f_blanco[2][0][1] = f_blanco_ul[2][1]
    f_blanco[2][0][2] = f_blanco_ul[2][2]########
    f_azul[0][0][0] = f_azul_ul[0][0]
    f_azul[0][0][1] = f_azul_ul[0][1]
    f_azul[0][0][2] = f_azul_ul[0][2]
    f_azul[1][0][0] = f_azul_ul[1][0]
    f_azul[1][0][1] = f_azul_ul[1][1]
    f_azul[1][0][2] = f_azul_ul[1][2]
    f_azul[2][0][0] = f_azul_ul[2][0]
    f_azul[2][0][1] = f_azul_ul[2][1]
    f_azul[2][0][2] = f_azul_ul[2][2]########
    f_naranja[0][0][0] = f_naranja_ul[0][0]
    f_naranja[0][0][1] = f_naranja_ul[0][1]
    f_naranja[0][0][2] = f_naranja_ul[0][2]
    f_naranja[1][0][0] = f_naranja_ul[1][0]
    f_naranja[1][0][1] = f_naranja_ul[1][1]
    f_naranja[1][0][2] = f_naranja_ul[1][2]
    f_naranja[2][0][0] = f_naranja_ul[2][0]
    f_naranja[2][0][1] = f_naranja_ul[2][1]
    f_naranja[2][0][2] = f_naranja_ul[2][2]########
    f_verde[0][0][0] = f_verde_ul[0][0]
    f_verde[0][0][1] = f_verde_ul[0][1]
    f_verde[0][0][2] = f_verde_ul[0][2]
    f_verde[1][0][0] = f_verde_ul[1][0]
    f_verde[1][0][1] = f_verde_ul[1][1]
    f_verde[1][0][2] = f_verde_ul[1][2]
    f_verde[2][0][0] = f_verde_ul[2][0]
    f_verde[2][0][1] = f_verde_ul[2][1]
    f_verde[2][0][2] = f_verde_ul[2][2]########
    f_rojo[0][0][0] = f_rojo_ul[0][0]
    f_rojo[0][0][1] = f_rojo_ul[0][1]
    f_rojo[0][0][2] = f_rojo_ul[0][2]
    f_rojo[1][0][0] = f_rojo_ul[1][0]
    f_rojo[1][0][1] = f_rojo_ul[1][1]
    f_rojo[1][0][2] = f_rojo_ul[1][2]
    f_rojo[2][0][0] = f_rojo_ul[2][0]
    f_rojo[2][0][1] = f_rojo_ul[2][1]
    f_rojo[2][0][2] = f_rojo_ul[2][2]

'''MOVER ABAJO A LA DERECHA'''

def abajo_derecha (f_blanco,f_azul,f_naranja,f_verde,f_rojo,f_amarillo):
    print("Cara blanca: ",f_blanco)
    f_azul_dr=[[[],[],[]],[[],[],[]],[[],[],[]]]
    f_azul_dr[0][0]=f_azul[0][0][0]        #9
    f_azul_dr[0][1]=f_azul[0][0][1]        #10
    f_azul_dr[0][2]=f_azul[0][0][2]        #11
    f_azul_dr[1][0]=f_azul[1][0][0]        #12
    f_azul_dr[1][1]=f_azul[1][0][1]        #13
    f_azul_dr[1][2]=f_azul[1][0][2]        #14
    f_azul_dr[2][0]=f_rojo[2][0][0]        #42
    f_azul_dr[2][1]=f_rojo[2][0][1]        #43
    f_azul_dr[2][2]=f_rojo[2][0][2]        #44
    print("Cara azul: ", f_azul_dr)
    f_naranja_dr=[[[],[],[]],[[],[],[]],[[],[],[]]]########
    f_naranja_dr[0][0]=f_naranja[0][0][0]    #18
    f_naranja_dr[0][1]=f_naranja[0][0][1]    #19
    f_naranja_dr[0][2]=f_naranja[0][0][2]    #20
    f_naranja_dr[1][0]=f_naranja[1][0][0]    #21
    f_naranja_dr[1][1]=f_naranja[1][0][1]    #22
    f_naranja_dr[1][2]=f_naranja[1][0][2]    #23
    f_naranja_dr[2][0]=f_azul[2][0][0]       #15
    f_naranja_dr[2][1]=f_azul[2][0][1]       #16
    f_naranja_dr[2][2]=f_azul[2][0][2]       #17
    print("Cara naranja: ", f_naranja_dr)
    f_verde_dr=[[[],[],[]],[[],[],[]],[[],[],[]]]########
    f_verde_dr[0][0]=f_verde[0][0][0]      #27
    f_verde_dr[0][1]=f_verde[0][0][1]      #28
    f_verde_dr[0][2]=f_verde[0][0][2]      #29
    f_verde_dr[1][0]=f_verde[1][0][0]      #30
    f_verde_dr[1][1]=f_verde[1][0][1]      #31
    f_verde_dr[1][2]=f_verde[1][0][2]      #32
    f_verde_dr[2][0]=f_naranja[2][0][0]    #24
    f_verde_dr[2][1]=f_naranja[2][0][1]    #25
    f_verde_dr[2][2]=f_naranja[2][0][2]    #26
    print("Cara verde: ",f_verde_dr)
    f_rojo_dr=[[[],[],[]],[[],[],[]],[[],[],[]]]########
    f_rojo_dr[0][0]=f_rojo[0][0][0]    #36
    f_rojo_dr[0][1]=f_rojo[0][0][1]    #37
    f_rojo_dr[0][2]=f_rojo[0][0][2]    #38
    f_rojo_dr[1][0]=f_rojo[1][0][0]    #39
    f_rojo_dr[1][1]=f_rojo[1][0][1]    #40
    f_rojo_dr[1][2]=f_rojo[1][0][2]    #41
    f_rojo_dr[2][0]=f_verde[2][0][0]   #33
    f_rojo_dr[2][1]=f_verde[2][0][1]   #34
    f_rojo_dr[2][2]=f_verde[2][0][2]   #35
    print("Cara roja: ",f_rojo_dr)
    f_amarillo_dr=[[[],[],[]],[[],[],[]],[[],[],[]]]########
    f_amarillo_dr[0][0]=f_amarillo[2][0][0]   #51
    f_amarillo_dr[0][1]=f_amarillo[1][0][0]   #48
    f_amarillo_dr[0][2]=f_amarillo[0][0][0]   #45
    f_amarillo_dr[1][0]=f_amarillo[2][0][1]   #52
    f_amarillo_dr[1][1]=f_amarillo[1][0][1]   #49
    f_amarillo_dr[1][2]=f_amarillo[0][0][1]   #46
    f_amarillo_dr[2][0]=f_amarillo[2][0][2]   #53
    f_amarillo_dr[2][1]=f_amarillo[1][0][2]   #50
    f_amarillo_dr[2][2]=f_amarillo[0][0][2]   #47
    print("Cara amarilla: ",f_amarillo_dr)

    f_amarillo[0][0][0] = f_amarillo_dr[0][0]
    f_amarillo[0][0][1] = f_amarillo_dr[0][1]
    f_amarillo[0][0][2] = f_amarillo_dr[0][2]
    f_amarillo[1][0][0] = f_amarillo_dr[1][0]
    f_amarillo[1][0][1] = f_amarillo_dr[1][1]
    f_amarillo[1][0][2] = f_amarillo_dr[1][2]
    f_amarillo[2][0][0] = f_amarillo_dr[2][0]
    f_amarillo[2][0][1] = f_amarillo_dr[2][1]
    f_amarillo[2][0][2] = f_amarillo_dr[2][2]########
    f_azul[0][0][0] = f_azul_dr[0][0]
    f_azul[0][0][1] = f_azul_dr[0][1]
    f_azul[0][0][2] = f_azul_dr[0][2]
    f_azul[1][0][0] = f_azul_dr[1][0]
    f_azul[1][0][1] = f_azul_dr[1][1]
    f_azul[1][0][2] = f_azul_dr[1][2]
    f_azul[2][0][0] = f_azul_dr[2][0]
    f_azul[2][0][1] = f_azul_dr[2][1]
    f_azul[2][0][2] = f_azul_dr[2][2]########
    f_naranja[0][0][0] = f_naranja_dr[0][0]
