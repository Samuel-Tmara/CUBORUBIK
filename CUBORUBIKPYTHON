def esquina_6(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo):
    if 6 in fila3_b:
        if 6 in [2][0][2]:
            derecha_abajo(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Derecha abajo")
            abajo_izquierda(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("abajo izquierda")
            derecha_arriba(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Derecha arriba")
            abajo_izquierda(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("abajo izquierda")
            direccion_antihoraria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Dirección antihoraria")
            abajo_derecha(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("abajo Derecha")
            direccion_horaria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Dirección horaria")

    if 6 in fila1_a:
        while f_azul[2][0][2] != 6:
            direccion_horaria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Dirección horaria")
        abajo_izquierda(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Abajo izquierda")
        abajo_izquierda(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Abajo izquierda")
        while f_azul[0][0][1] != 10:
