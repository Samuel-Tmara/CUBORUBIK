def esquina_2(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo):

    if 2 in fila3_b:
        if 2 in [2][0][0]:
            direccion_antihoraria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Dirección antihoraria")
            abajo_derecha(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Abajo-derecha")
            direccion_antihoraria_verde(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Dirección antihoraria verde")
            abajo_derecha(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Abajo-derecha")
            direccion_horaria_verde(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Dirección horaria verde")
            direccion_horaria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Dirección horaria")
        if 2 in [2][0][2]:
            derecha_abajo(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("derecha-abajo")
            abajo_derecha(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("abajo-derecha")
            derecha_arriba(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("derecha-arriba")
            abajo_derecha(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("abajo-derecha")
            abajo_derecha(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("abajo-derecha")
            derecha_arriba(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("derecha-arriba")
            abajo_izquierda(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("abajo-izquierda")
            derecha_abajo(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("derecha-abajo")

    if 2 in fila1_a:
        while f_azul[2][0][2]!= 2:
            direccion_horaria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Dirección horaria")
        direccion_antihoraria_verde(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Dirección antihoraria verde")
        abajo_derecha(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Abajo-derecha")
        direccion_horaria_verde(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Dirección horaria verde")
        while f_azul[0][0][1] != 10:
            direccion_horaria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Dirección horaria")
    elif 2 in fila2_a:
        while f_azul[2][0][2]!= 2:
            direccion_horaria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Dirección horaria")
        direccion_antihoraria_verde(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Dirección antihoraria verde")
        abajo_derecha(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Abajo-derecha")
        direccion_horaria_verde(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
        print("Dirección horaria verde")
        while f_azul[0][0][1] != 10:
            direccion_horaria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Dirección horaria")
    elif 2 in fila3_a:
        while f_azul[2][0][2]!= 2:
            direccion_horaria(f_blanco, f_azul, f_naranja, f_verde, f_rojo, f_amarillo)
            print("Dirección horaria")
