'''GIRO DIRECCIÓN HORARIA'''

def direccion_horaria (f_blanco,f_azul,f_naranja,f_verde,f_rojo,f_amarillo):
    f_blanco_dh=[[[],[],[]],[[],[],[]],[[],[],[]]]
    f_blanco_dh[0][0]=f_blanco[0][0] [0]    #0
    f_blanco_dh[0][1]=f_blanco[0][0] [1]    #1
    f_blanco_dh[0][2]=f_blanco[0][0] [2]    #2
    f_blanco_dh[1][0]=f_blanco[1][0] [0]    #3
    f_blanco_dh[1][1]=f_blanco[1][0] [1]    #4
    f_blanco_dh[1][2]=f_blanco[1][0] [2]    #5
    f_blanco_dh[2][0]=f_rojo[2][0] [2]      #44
    f_blanco_dh[2][1]=f_rojo[1][0] [2]      #41
    f_blanco_dh[2][2]=f_rojo[0][0] [2]      #38
    print("Cara blanca: ",f_blanco_dh)
    f_azul_dh=[[[],[],[]],[[],[],[]],[[],[],[]]]########
    f_azul_dh[0][0]=f_azul[2][0] [0]    #15
    f_azul_dh[0][1]=f_azul[1][0] [0]    #12
    f_azul_dh[0][2]=f_azul[0][0] [0]    #9
    f_azul_dh[1][0]=f_azul[2][0] [1]    #16
    f_azul_dh[1][1]=f_azul[1][0] [1]    #13
    f_azul_dh[1][2]=f_azul[0][0] [1]    #10
    f_azul_dh[2][0]=f_azul[2][0] [2]    #17
    f_azul_dh[2][1]=f_azul[1][0] [2]    #14
    f_azul_dh[2][2]=f_azul[0][0] [2]    #11
    print("Cara azul: ", f_azul_dh)
    f_naranja_dh=[[[],[],[]],[[],[],[]],[[],[],[]]]########
    f_naranja_dh[0][0]=f_blanco[2][0] [0]    #6
    f_naranja_dh[0][1]=f_naranja[0][0] [1]   #19
    f_naranja_dh[0][2]=f_naranja[0][0] [2]   #20
    f_naranja_dh[1][0]=f_blanco[2][0] [1]    #7
    f_naranja_dh[1][1]=f_naranja[1][0] [1]   #22
    f_naranja_dh[1][2]=f_naranja[1][0] [2]   #23
    f_naranja_dh[2][0]=f_blanco[2][0] [2]    #8
    f_naranja_dh[2][1]=f_naranja[2][0] [1]   #25
    f_naranja_dh[2][2]=f_naranja[2][0] [2]   #26
    print("Cara naranja: ", f_naranja_dh)
    print("Cara verde: ",f_verde)
    f_rojo_dh=[[[],[],[]],[[],[],[]],[[],[],[]]]########
    f_rojo_dh[0][0]=f_rojo[0][0] [0]        #36
    f_rojo_dh[0][1]=f_rojo[0][0] [1]        #37
    f_rojo_dh[0][2]=f_amarillo[0][0] [0]    #45
    f_rojo_dh[1][0]=f_rojo[1][0] [0]        #39
    f_rojo_dh[1][1]=f_rojo[1][0] [1]        #40
    f_rojo_dh[1][2]=f_amarillo[0][0] [1]    #46
    f_rojo_dh[2][0]=f_rojo[2][0] [0]        #42
    f_rojo_dh[2][1]=f_rojo[2][0] [1]        #43
    f_rojo_dh[2][2]=f_amarillo[0][0] [2]    #47
    print("Cara roja: ",f_rojo_dh)
    f_amarillo_dh=[[[],[],[]],[[],[],[]],[[],[],[]]]########
    f_amarillo_dh[0][0]=f_naranja[2][0] [0]   #24
    f_amarillo_dh[0][1]=f_naranja[1][0] [0]   #21
    f_amarillo_dh[0][2]=f_naranja[0][0] [0]   #18
    f_amarillo_dh[1][0]=f_amarillo[1][0] [0]  #48
    f_amarillo_dh[1][1]=f_amarillo[1][0] [1]  #49
    f_amarillo_dh[1][2]=f_amarillo[1][0] [2]  #50
    f_amarillo_dh[2][0]=f_amarillo[2][0] [0]  #51
    f_amarillo_dh[2][1]=f_amarillo[2][0] [1]  #52
    f_amarillo_dh[2][2]=f_amarillo[2][0] [2]  #53
    print("Cara amarilla: ",f_amarillo_dh)

    f_blanco[0][0] [0] = f_blanco_dh[0][0]
