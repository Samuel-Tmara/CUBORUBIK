    f_rojo_dl[2][1]=f_azul[2][0][1]    #34
    f_rojo_dl[2][2]=f_azul[2][0][2]    #35
    print("Cara roja: ",f_rojo_dl)
    f_amarillo_dl=[[[],[],[]],[[],[],[]],[[],[],[]]]########
