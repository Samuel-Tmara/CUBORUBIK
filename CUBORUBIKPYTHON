    f_azul[1][0] [0] = f_azul_dh[1][0]
    f_azul[1][0] [1] = f_azul_dh[1][1]
    f_azul[1][0] [2] = f_azul_dh[1][2]
    f_azul[2][0] [0] = f_azul_dh[2][0]
