    f_verde_dl[1][0]=f_verde[1][0][0]      #30
    f_verde_dl[1][1]=f_verde[1][0][1]      #31
    f_verde_dl[1][2]=f_verde[1][0][2]      #32
    f_verde_dl[2][0]=f_rojo[2][0][0]       #24
    f_verde_dl[2][1]=f_rojo[2][0][1]       #25
    f_verde_dl[2][2]=f_rojo[2][0][2]       #26
